#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/payflink/PayflinkHomepage/Entwicklung/gaston-webpage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/payflink/PayflinkHomepage/Entwicklung/gaston-webpage/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@netlify+esbuild@0.14.25/node_modules/@netlify/esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../../../../@netlify+esbuild@0.14.25/node_modules/@netlify/esbuild/bin/esbuild" "$@"
fi
