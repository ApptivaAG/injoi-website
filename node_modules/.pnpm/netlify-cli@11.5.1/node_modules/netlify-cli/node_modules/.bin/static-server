#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../static-server@2.2.1/node_modules/static-server/bin/static-server.js" "$@"
else
  exec node  "$basedir/../../../../../static-server@2.2.1/node_modules/static-server/bin/static-server.js" "$@"
fi
