#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@netlify+build@27.17.1/node_modules/@netlify/build/src/core/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@netlify+build@27.17.1/node_modules/@netlify/build/src/core/bin.js" "$@"
fi
